#include <stdio.h>

int num_1, num_2, num_3, may, men;

int main(void){
	printf("Detector de numero de mayor valor");
	printf("Pirmer valor\n");
	scanf("%d", &num_1);
	printf("Segundo valor\n");
	scanf("%d", &num_2);
	printf("Tercer valor\n");
	scanf("%d", &num_3);
	
	if (num_1 > num_2){
		if (num_1 > num_3){
			may = num_1;
		}
		if (num_2 > num_3){
			may = num_2;
		}else{
			may = num_3;
		}
		men = 0;
	}
	
	printf("El mayor es %d", may);
	printf("El menor es %d", may);
}
